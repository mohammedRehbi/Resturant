<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Resturant">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.example.Resturant.prfitsActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Resturant.ReportShowActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Resturant.deliveryActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Resturant.ChefActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Resturant.reportActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Resturant.MealViewActivity"
            android:exported="false" />
        <activity
            android:name=".ReceptionistActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Resturant.PaymentActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Resturant.CurrentOrderActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Resturant.CheckInResultActivity"
            android:exported="true" />
