            android:name="com.example.Resturant.CheckInActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Resturant.MealActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Resturant.HomeActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Resturant.LoginActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Resturant.SignupActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Resturant.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>